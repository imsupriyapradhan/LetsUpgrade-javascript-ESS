  <meta charset="utf-8">
  <title>Assignment 1</title>
</head>
<body>
  <!-- Create a two variables and perform all arithmatic (Addition, Substraction..etc) operation and print the result in the console using templating syntax -->
  <script>
    let num1 = 20;
    let num2 = 40;
    // Addition
    add = num1 + num2;
    console.log(`Addition ${add}`);
    //Substraction
    sub = num1 - num2;
    console.log(`Substraction ${sub}`);
    //Multiplication
    multi = num1 * num2;
    console.log(`Multiplication ${multi}`);
    //Division
    div = num1 / num2;
    console.log(`Division ${div}`);
    //Reminder
    rem = num1 % num2;
    console.log(`Reminder ${rem}`);
  </script>
</body>
</html>
